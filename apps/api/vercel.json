{
  "version": 2,
  "buildCommand": "npx prisma generate && pnpm run build",
  "installCommand": "pnpm install",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS"
      ]
    }
  ],
  "functions": {
    "dist/main.js": {
      "maxDuration": 30
    }
  }
}
